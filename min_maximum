#define _CRT_SECURE_NO_WARNINGS
#include <iostream>
#include <vector>
#include <string>
#include <algorithm>


using namespace std;

vector<int> splitSort2(string s, string div) {
	vector <int> v;

	int start = 0;
	int d = s.find(div,start);

	while (d != -1) {
		v.push_back(stoi(s.substr(start,d-start)));
		start = d + 1;
		d = s.find(div,start);
	}

	v.push_back(stoi(s.substr(start, d - start)));

	sort(v.begin(), v.end(), [](int a, int b){
		return a < b;
	});
	return v;
}


int main() {

	int n=0;
	cin >> n;
    
	cin.ignore();
	string s;
	getline(cin, s, '\n');

	vector <int> v2 = splitSort2(s, " ");

	cout << v2[0] << " " << v2[v2.size() - 1];
	return 0;
}
