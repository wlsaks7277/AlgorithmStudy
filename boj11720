#include <vector>
#include <string>
#include <iostream>


using namespace std;

void solution(string t,int n) {
	int sum = 0;
	for (int i = 0; i < n; i++) {
		sum += t[i]-'0';
	}
	cout << sum;
}

int main() {

	int n = 0;
	string t = "";
	cin >> n;


	//cout << n;
	cin.ignore();
	getline(cin, t);
	solution(t,n);

}
